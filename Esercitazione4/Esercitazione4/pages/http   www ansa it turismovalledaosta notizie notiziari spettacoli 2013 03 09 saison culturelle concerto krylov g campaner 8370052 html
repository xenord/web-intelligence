var headLoaded=true;
 
function getMeta(metaname) {
    var metas = $("meta");
    var i;
    for (i = 0; i < metas.length; i++)
    	if (metas[i].getAttribute("NAME") == metaname)
	  break;
    var content = metas[i].getAttribute("CONTENT");
    return content;
}
