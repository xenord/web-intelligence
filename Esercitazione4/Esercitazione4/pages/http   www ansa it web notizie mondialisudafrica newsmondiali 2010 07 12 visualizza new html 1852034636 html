// Funzione makeAD 1.0b
var TiscaliAdRandomNumber=Math.floor(Math.random()*10000000000);
var TiscaliAdTileNumber=0;

var TiscaliAdArea='ansa.mondiali'; //getAD(document.location, adv_banner_home, adv_banner_default, adv_banner);
var TiscaliAdInternal=false;//isInternal(document.location);

function makeAD(area,rich,pos,size,kw) {
  if (TiscaliAdInternal==true) {
    if (size == '300x250') {
      size = '300x250,300x600';
    }
  }

  //document.write('<scr'+'ipt id="mk'+size.replace(/,.+$/, '')+pos+'" src="http://ad.it.doubleclick.net/adj/'+canale+(area?'/'+area:'')+(rich?';dcopt=ist':'')+';pos='+pos+(kw?';kw='+kw:'')+';tile='+(++TiscaliAdTileNumber)+';sz='+size+';ord='+TiscaliAdRandomNumber+'?"></scr'+'ipt>');
  document.write('<scr'+'ipt id="mk'+size.replace(/,.+$/, '')+pos+'" src="http://ad.it.doubleclick.net/adj/'+area+(rich?';dcopt=ist':'')+';pos='+pos+(kw?';kw='+kw:'')+';tile='+(++TiscaliAdTileNumber)+';sz='+size+';ord='+TiscaliAdRandomNumber+'?"></scr'+'ipt>');
}

// Funzione moveBanner 1.0
var isOpera = (navigator.userAgent.indexOf('Opera') > -1)?1:0;
function moveBanner(id) {
  var target = document.getElementById('banner'+id);
  var source = document.getElementById('banner'+id+'hide');

  if (target && source) {
    //source.removeChild(document.getElementById('mk'+id));
    //var ad_script = document.getElementById('mk'+id);
    //if (ad_script) {
    //  source.removeChild(ad_script);
    //}
    
    if (isOpera) {
      target.innerHTML=source.innerHTML;
      source.innerHTML='';
    } else {
      target.innerHTML='';
      target.appendChild(source);
      source.style.display='block';   
    }
  }
}

function writeBanner(id, val) {
  var target = document.getElementById(id);
  if (target) {
  	target.innerHTML = val;
  }
}

function testAD(id,Area,rich,pos,size,kw) {
  var target = document.getElementById('banner'+id);
  if (target) {
    makeAD(Area,rich,pos,size,kw);
  }
}

function getMakeAD(area,rich,pos,size,kw) {
  if (TiscaliAdInternal==true) {
    if (size == '300x250') {
      size = '300x250,300x600';
    }
  }
  return '<scr'+'ipt id="mk'+size.replace(/,.+$/, '')+pos+'" src="http://ad.it.doubleclick.net/adj/'+area+(rich?';dcopt=ist':'')+';pos='+pos+(kw?';kw='+kw:'')+';tile='+(++TiscaliAdTileNumber)+';sz='+size+';ord='+TiscaliAdRandomNumber+'?"></scr'+'ipt>';
}

